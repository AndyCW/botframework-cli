{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "equipment": {
            "type": "string",
            "enum": [
                "echo bot",
                "air conditioner",
                "lights"
            ],
            "$entities": [
                "equipmentEntity"
            ]
        },
        "StartConversation": {
            "type": "string",
            "$entities": [
                "Confirmation"
            ]
        },
        "CancelConfirmation": {
            "type": "string",
            "$entities": [
                "Confirmation"
            ]
        },
        "ChangePropertyConfirmation": {
            "type": "string",
            "$entities": [
                "Confirmation"
            ]
        },
        "CompleteConfirmation": {
            "type": "string",
            "$entities": [
                "Confirmation",
                "PROPERTYName"
            ]
        },
        "PropertyToChange": {
            "type": "string",
            "enum": "properties",
            "$entities": [
                "PROPERTYName"
            ]
        },
        "PropertyToRemember": {
            "type": "string",
            "enum": "properties",
            "$entities": [
                "PROPERTYName"
            ]
        }
    },
    "required": [
        "equipment"
    ],
    "$expectedOnly": [
        "Confirmation",
        "PROPERTYName",
        "utterance",
        "keyPhrase",
        "personName"
    ],
    "$public": [
        "equipment",
        "StartConversation"
    ],
    "definitions": {
        "PROPERTYName": {
            "type": "string",
            "enum": "properties",
            "$entities": [
                "PROPERTYName"
            ]
        }
    }
}