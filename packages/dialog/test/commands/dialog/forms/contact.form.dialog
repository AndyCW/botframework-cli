{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "HomeStreet": { "type": "string" },
        "HomeCity": { "type": "string" },
        "HomeState": { 
            "type": "string",
            "$mappings": [ "STATE" ]
        },
        "HomeZip": { 
            "type": "string",
            "pattern": "^[0-9]{5}(?:-[0-9]{4})?$"
        },
        "HomeLatitude": { "type": "number" },
        "HomeLongitude": { "type": "number" },
        "HomePhone": { 
            "type": "string", 
            "pattern": "^(\\([0-9]{3}\\)|[0-9]{3}-)[0-9]{3}-[0-9]{4}$"
        },

        "WorkStreet": { "type": "string" },
        "WorkCity": { "type": "string" },
        "WorkState": { 
            "type": "string",
            "$mappings": [ "STATE" ]
        },
        "WomeZip": { 
            "type": "string",
            "pattern": "^[0-9]{5}(?:-[0-9]{4})?$"
        },
        "WorkLatitude": { "type": "number" },
        "WorkLongitude": { "type": "number" },
        "WorkPhone": { 
            "type": "string", 
            "pattern": "^(\\([0-9]{3}\\)|[0-9]{3}-)[0-9]{3}-[0-9]{4}$"
        }
    }
}