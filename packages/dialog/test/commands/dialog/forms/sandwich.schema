{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "petId": {
            "type": "number",
            "description": "ID of pet that needs to be updated"
        },
        "name": {
            "type": "string",
            "description": "Updated name of the pet"
        },
        "status": {
            "type": "string",
            "description": "Updated status of the pet"
        }
    },
    "required": [
        "petId",
        "name",
        "status"
    ],
    "$requires": [
        "standard.schema"
    ],
    "swaggerApi": "petstore.swagger.io/v2//pet/{petId}",
    "swaggerMethod": "post",
    "swaggerResponse": "dialog.response"
}
